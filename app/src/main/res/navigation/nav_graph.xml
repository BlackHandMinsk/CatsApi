<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.catsapi.ui.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_catInfoFragment"
            app:destination="@id/catInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/catInfoFragment"
        android:name="com.example.catsapi.ui.CatInfoFragment"
        android:label="fragment_cat_info"
        tools:layout="@layout/fragment_cat_info" >
        <action
            android:id="@+id/action_catInfoFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <argument
            android:name="url"
            app:argType="string" />
    </fragment>
</navigation>